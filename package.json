{
  "name": "node-md-meta-cataloger",
  "version": "1.0.2",
  "description": "A module and CLI for parsing Markdown files (including YAML Front Matter metadata).",
  "keywords": [
    "markdown",
    "md",
    "yaml",
    "metadata",
    "parser"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/lightpohl/node-md-meta-cataloger.git"
  },
  "main": "./src/index.js",
  "bin": "./bin/bin.js",
  "files": [
    "src",
    "bin"
  ],
  "scripts": {
    "precommit": "lint-staged",
    "prepush": "npm run lint && npm run test",
    "lint": "eslint --ext .js .",
    "test": "jest",
    "release": "standard-version"
  },
  "lint-staged": {
    "*.{js,json,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "author": "Joshua Pohl",
  "license": "MIT",
  "dependencies": {
    "commander": "^2.17.1",
    "glob": "^7.1.2",
    "js-yaml": "^3.12.0"
  },
  "devDependencies": {
    "eslint": "^5.3.0",
    "eslint-config-defaults": "^9.0.0",
    "eslint-config-prettier": "^3.0.1",
    "husky": "^0.14.3",
    "jest": "^23.5.0",
    "lint-staged": "^7.2.2",
    "prettier": "1.14.2",
    "standard-version": "^4.4.0"
  }
}
